Option Compare Database
Option Explicit
Private Sub cmdUpdateChart_Click()
    
    'Make sure frm_Demo is open
    If IsFormOpen("frm_Demo") = False Then
        DoCmd.OpenForm "frm_Demo", acNormal
    End If
    
    Forms!frm_Demo.SetFocus
    DoEvents
    Forms!frm_Demo.cmdCreateChart_Click
End Sub
Private Sub fraChartType_AfterUpdate()
    Select Case Me.fraChartType
        Case chChartType.line, chChartType.Bar, chChartType.HorizontalBar, chChartType.Bubble, chChartType.Scatter
            Me.chkShowYAxis = True
            Me.chkShowXAxis = True
        Case chChartType.Radar, chChartType.Doughnut, chChartType.Pie, chChartType.PolarArea
            Me.chkShowYAxis = False
            Me.chkShowXAxis = False
        Case Else
            'Do Nothing
    End Select
End Sub
Private Sub cmdSaveChartJs_Click()
    If BAC.Chart.ChartJS.SaveToDisk = True Then
        MsgBox "Chart.js successfully saved to hdd.", vbInformation, "Better Access Charts"
    Else
        MsgBox "Something went wrong!", vbExclamation, "Better Access Charts"
    End If
End Sub
Private Sub cmdSaveColorSchema_Click()
    If BAC.Chart.ChartJS.Plugin_DataLabel.SavePluginToDisk = True Then
        MsgBox "Files related to color schemes plug-in " & vbCrLf & _
               "successfully saved to hdd.", vbInformation, "Better Access Charts"
    Else
        MsgBox "Something went wrong!", vbExclamation, "Better Access Charts"
    End If
End Sub
Private Sub cmdSaveDataLabel_Click()
    If BAC.Chart.ChartJS.Plugin_DataLabel.SavePluginToDisk = True Then
        MsgBox "Chart.js data label plug-in " & vbCrLf & _
               "successfully saved to hdd.", vbInformation, "Better Access Charts"
    Else
        MsgBox "Something went wrong!", vbExclamation, "Better Access Charts"
    End If
End Sub
